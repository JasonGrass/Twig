<UserControl
    x:Class="Twig.Wpf.Example.Views.SecondSubView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Twig.Wpf.Example.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModels="clr-namespace:Twig.Wpf.Example.ViewModels"
    xmlns:prism="http://prismlibrary.com/"
    d:DesignHeight="450"
    d:DesignWidth="800"
    d:DataContext="{d:DesignInstance viewModels:SecondSubViewModel}"
    prism:ViewModelLocator.AutoWireViewModel="True"
    mc:Ignorable="d">
    <Grid>

        <Button
            Width="100" Height="36" HorizontalAlignment="Center"
            VerticalAlignment="Center"
            Command="{Binding ShowDataCommand}">
            Show Data
        </Button>
    </Grid>
</UserControl>
